{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Feline Finder (Chrome)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port=8080",
        "--web-hostname=localhost"
      ],
      "console": "debugConsole",
      "cwd": "${workspaceFolder}/feline_finder",
      "deviceId": "chrome"
    },
    {
      "name": "Feline Finder (iOS Simulator)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "ios"
      ],
      "console": "debugConsole"
    },
    {
      "name": "Feline Finder (Android)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "android"
      ],
      "console": "debugConsole"
    },
    {
      "name": "Feline Finder (macOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "macos"
      ],
      "console": "debugConsole"
    },
    {
      "name": "Dart: Attach to Process",
      "type": "dart",
      "request": "attach"
    }
  ]
}